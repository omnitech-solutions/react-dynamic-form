{
  "name": "react-dynamic-form",
  "private": true,
  "version": "0.0.1",
  "main": "./dist/react-dynamic-form.cjs",
  "module": "./dist/react-dynamic-form.mjs",
  "exports": {
    ".": {
      "require": "./dist/react-dynamic-form.cjs",
      "import": "./dist/react-dynamic-form.mjs"
    }
  },
  "scripts": {
    "prepare": "husky install && husky set .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit",
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "lint": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format": "npm run format:scripts && npm run format:styles",
    "cy": "cypress open",
    "uninstall-husky": "npm uninstall husky --no-save && git config --unset core.hooksPath && npx rimraf .husky"
  },
  "devDependencies": {
    "@babel/runtime-corejs2": "^7.23.1",
    "@cypress/sinon-chai": "^2.9.1",
    "@testing-library/react": "^14.0.0",
    "@types/chai": "^4.3.6",
    "@types/chai-arrays": "^2.0.1",
    "@types/chai-as-promised": "^7.1.6",
    "@types/chai-datetime": "^0.0.37",
    "@types/deep-equal-in-any-order": "^1.0.1",
    "@types/dirty-chai": "^2.0.2",
    "@types/html": "^1.0.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.1",
    "@types/react-test-renderer": "^18.0.3",
    "@types/sinon": "^10.0.17",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "@vitest/coverage-v8": "^0.34.6",
    "chai": "^4.3.10",
    "chai-arrays": "^2.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-datetime": "^1.8.0",
    "chai-jest-diff": "^1.0.2",
    "chai-sorted": "^0.2.0",
    "chai-url": "^1.0.4",
    "core-js": "^3.33.0",
    "cypress": "^13.3.0",
    "deep-equal-in-any-order": "^2.0.6",
    "dirty-chai": "^2.0.1",
    "dts-bundle-generator": "^7.2.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "global": "^4.4.0",
    "html": "^1.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "json-rules-engine-simplified": "^0.1.17",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sinon": "^16.0.0",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@oc-tech/lodash-ext": "^0.0.6",
    "@rjsf/antd": "^5.13.0",
    "@rjsf/core": "^5.13.0",
    "@rjsf/utils": "^5.13.0",
    "@rjsf/validator-ajv8": "^5.13.0",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.24",
    "antd": "^5.9.4",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rjsf-conditionals": "^1.5.0"
  }
}
